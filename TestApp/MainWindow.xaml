<Window x:Name="Calculator" x:Class="TestApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="375" Width="500">
	<StackPanel>

		<TextBlock Background="Green">Hello World</TextBlock>
		<TextBlock Text="{Binding LogMessages.Count}"></TextBlock>
		<ListView ItemsSource="{Binding LogMessages}"></ListView>
		
	</StackPanel>
</Window>
